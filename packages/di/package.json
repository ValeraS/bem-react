{
  "name": "@bem-react/di",
  "version": "2.2.1",
  "description": "BEM React Dependency Injection",
  "homepage": "https://github.com/bem/bem-react/tree/master/packages/di",
  "repository": "https://github.com/bem/bem-react",
  "keywords": ["bem", "level", "dependency", "di", "dependency injection", "react"],
  "main": "index.js",
  "typings": "di.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": ["build", "di.d.ts"],
  "license": "MPL-2.0",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "node ../../scripts/rollup/build.js",
    "lint": "cross-env __DEV__=1 tsc -p . --noEmit && tslint -p . -t stylish",
    "unit": "mocha --opts ../../mocha.opts --",
    "test": "nyc npm run unit test/*.test.tsx"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "nyc": {
    "include": ["di.tsx"],
    "extension": [".ts", ".tsx"],
    "reporter": ["text", "html"],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
